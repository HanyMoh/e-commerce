<!DOCTYPE html>
<html>
<head>
  <title>NasNav API documentation | NasNav</title>
  <%= stylesheet_link_tag 'api', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'api', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='//fonts.googleapis.com/css?family=Droid+Sans:400,700' rel='stylesheet' type='text/css'/>
  <link href='/ipa-swagger/swagger-ui/css/highlight.default.css' media='screen' rel='stylesheet' type='text/css'/>
  <link href='/ipa-swagger/swagger-ui/css/screen.css' media='screen' rel='stylesheet' type='text/css'/>
  <script type="text/javascript" src="/ipa-swagger/swagger-ui/lib/shred.bundle.js" /></script>
  <script src='/ipa-swagger/swagger-ui/lib/jquery-1.8.0.min.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/jquery.slideto.min.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/jquery.wiggle.min.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/jquery.ba-bbq.min.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/handlebars-1.0.0.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/underscore-min.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/backbone-min.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/swagger.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/swagger-ui.js' type='text/javascript'></script>
  <script src='/ipa-swagger/swagger-ui/lib/highlight.7.3.pack.js' type='text/javascript'></script>
  <script type="text/javascript">
    $(function () {
      window.swaggerUi = new SwaggerUi({
      url: "/ipa-swagger/api-docs.json",
      dom_id: "swagger-ui-container",
      supportedSubmitMethods: ['get', 'post', 'put', 'delete'],
      onComplete: function(swaggerApi, swaggerUi){
        if(console) {
          console.log("Loaded SwaggerUI")
        }
        $('pre code').each(function(i, e) {hljs.highlightBlock(e)});
      },
      onFailure: function(data) {
        if(console) {
          console.log("Unable to Load SwaggerUI");
          console.log(data);
        }
      },
      docExpansion: "none"
    });

    $('#input_apiKey').change(function() {
      var key = $('#input_apiKey')[0].value;
      console.log("key: " + key);
      if(key && key.trim() != "") {
        console.log("added key " + key);
        window.authorizations.add("key", new ApiKeyAuthorization("api_key", key, "query"));
      }
    })
    window.swaggerUi.load();
  });

  </script>
</head>

<body>
  <%= yield %> 
</body>
</html>
