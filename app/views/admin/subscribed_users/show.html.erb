<!-- Page Title -->
<div class="page-title-bar">
  <div class="row">
    <div class="col-xs-6"><h2><%= format_name(@subscribed_user.email) %></h2></div>
  </div>
</div>

<!-- Page Breadcrumb -->
<div class="page-breadcrumb">
  <div class="row">
    <div class="col-sm-7">
      <%= link_to admin_subscribed_users_path, class: 'back-angle' do %>
        <i class='fa fa-long-arrow-left'></i> Back To Subscribed Users
      <% end %>
    </div>
    <div class="col-sm-5">
      <ol class="breadcrumb">
          <li class="breadcrumb-item">
              <%= link_to 'Dashboard', admin_path %>
          </li>
          <li class="breadcrumb-item">
              <%= link_to 'Subscribed Users',admin_subscribed_users_path %>
          </li>
        <li class="breadcrumb-item active"><a href="javascript:void(0)"><%= format_name(@subscribed_user.email) %></a></li>
      </ol>
    </div>
  </div>
</div>

<div class="start-content">
  <div class="view-brand">
    <div class="brand-item">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-2"><h4 class="item-title">Email</h4></div>
          <div class="col-md-10"><p class="item-info"><%=@subscribed_user.try(:email)%></p></div>
        </div>
      </div>
    </div>
    <div class="brand-item">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-2"><h4 class="item-title">Shop</h4></div>
          <div class="col-md-10">
            <p class="item-info"><%= @subscribed_user.shop&.name %></p>
          </div>
        </div>
      </div>
    </div>
    <div class="brand-item">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-2"><h4 class="item-title">Brand</h4></div>
          <div class="col-md-10">
            <p class="item-info"><%= @subscribed_user.shop&.brand&.name %></p>
          </div>
        </div>
      </div>
    </div>
    <div class="brand-item">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-2"><h4 class="item-title">Organization</h4></div>
          <div class="col-md-10"><p class="item-info"><%= @subscribed_user.organization&.name %></p></div>
        </div>
      </div>
    </div>
    <div class="brand-item">
      <div class="row">
        <div class="col-md-12">
          <div class="col-md-2"><h4 class="item-title">Subscribed At</h4></div>
          <div class="col-md-10"><p class="item-info"><%= @subscribed_user.created_at.strftime("%Y-%m-%d %H:%M %p") %></p></div>
        </div>
      </div>
    </div>
  </div>
</div>

