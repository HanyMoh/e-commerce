    <!-- Custom Theme JavaScript -->
    <script>
        $(document).ready(function () {


        Morris.Bar({
            element: 'online-offline-users',
            data: [{
                hour: '8AM',
                Online: 50,
                Offline: 14
            }, {
                hour: '10AM',
                Online: 2,
                Offline: 30
            }, {
                hour: '12AM',
                Online: 56,
                Offline: 99
            }, {
                hour: '1PM',
                Online: 1,
                Offline: 2
            }, {
                hour: '3PM',
                Online: 5,
                Offline: 8
            }, {
                hour: '5PM',
                Online: 10,
                Offline: 14
            }, {
                hour: '7PM',
                Online: 34,
                Offline: 55
            }, {
                hour: '9PM',
                Online: 29,
                Offline: 34
            }, {
                hour: '11PM',
                Online: 33,
                Offline: 22
            }, {
                hour: '12PM',
                Online: 54,
                Offline: 112
            }, {
                hour: '2AM',
                Online: 50,
                Offline: 14
            }, {
                hour: '5AM',
                Online: 50,
                Offline: 14
            }
            ],
            xkey: 'hour',
            //xLabels:"period",
            xLabelAngle: 35,
            ykeys: ['Online', 'Offline'],
            labels: ['Online', 'Offline'],
            pointSize: 2,
            fillOpacity: 0,
            barColors:['#00bfc7', '#fb9678'],
            gridLineColor: '#e0e0e0',
            lineWidth: 2,
            hideHover: 'auto',
            resize: true

        });



        });


    </script>





</body>
