<body>
        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="bg-title">
                    <!-- .page title -->
                    <ol class="breadcrumb">
                        <li><a href="#">Dashboard</a></li>
                        <li class="active">you are here</li>
                    </ol>

                    <h4 class="page-title">User Data Analytics</h4>
                    <select class="form-control pull-right custom-select" style="width: 150px">
                        <option>Today</option>
                        <option>Yesterday</option>
                        <option>Last Week</option>
                        <option>Last Month</option>
                        <option>Last Year</option>
                    </select>


                    <!--<a href="#" class="btn btn-outline btn-rounded btn-primary m-l-10">Action Button</a>-->
                    <!-- /.page title -->
                </div>











                <!-- Start of the content page -->



                <div class="row">
                    <div class="col-md-12">
                        <div class="white-box">
                            <div class="box-title">
                                <h3 class="inline">Chart</h3>

                                <div class="inline pull-right">
                                    <ul class="list-inline">
                                        <li class="m-r-15"><h5><i class="fa fa-circle m-r-5" style="color: #00bfc7;"></i>Store Visit</h5> </li>
                                        <li><h5><i class="fa fa-circle m-r-5" style="color: #fb9678;"></i>VR Visit</h5></li>
                                    </ul>
                                </div>

                            </div>
                            <div id="online-offline-users" style="height: 300px;"></div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-md-6">
                        <div class="white-box">
                            <div class="box-title">
                                <h3 class="inline">Store Visits <%= @real_visits.count || 0 %></h3>
                            </div>


                            <div class="row sales-report">
                                <div class="col-md-6 col-sm-6 col-xs-6">
                                    <h2>Today</h2>
                                    <p>Users Live interacts with Beacons</p>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 ">
                                    <h1 class="text-right text-success m-t-20">32</h1> </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table ">
                                    <thead>
                                    <tr>
                                        <th>User ID</th>
                                        <th>First Seen</th>
                                        <th>Last Seen</th>
                                        <th>Beacon</th>
                                    </tr>
                                    </thead>
                                    <tbody>
									<% @real_visits.each do |rv| %>
										<tr>
											<td class="btn-link"><a href="#"><%= rv['user_id'] %></a></td>
											<td><%= rv['first_seen'] %></td>
											<td><%= rv['last_seen'] %></td>
											<td><a href="#"><%= rv['beacon']['name'] %></a></td>
										</tr>
									<% end %>
                                    </tbody>
                                </table>
                                <a href="#" class="btn btn-block btn-default">View All Today's online User</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="white-box">
                            <div class="box-title">
                                <h3>VR Visits <%= @vr_visits.count %></h3>
                            </div>


                            <div class="row sales-report">
                                <div class="col-md-6 col-sm-6 col-xs-6">
                                    <h2>Today</h2>
                                    <p>Users Live interacts with Beacons</p>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-6 ">
                                    <h1 class="text-right text-success m-t-20">32</h1> </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table ">
                                    <thead>
                                    <tr>
                                        <th>User ID</th>
                                        <th>First Seen</th>
                                        <th>Last Seen</th>
                                        <th>Spent Period</th>
                                    </tr>
                                    </thead>
                                    <tbody>
									<% @vr_visits.each do |vrv| %>
                                    <tr>
                                        <td class="btn-link"><a href="#"><%= vrv.user_id %></a></td>
                                        <td><%= vrv.first_seen %></td>
                                        <td><%= vrv.last_seen %></td>
                                        <td><a href="#"><%= vrv.spent_period %></a></td>
                                    </tr>
									<% end %>
                                    </tbody>
                                </table>
                                <a href="#" class="btn btn-block btn-default">View All Today's online User</a>
                            </div>
                        </div>
                    </div>
                </div>






                <!-- End of the content page -->
            </div>
            <!-- /.container-fluid -->
            <footer class="footer"> 2017 &copy; NassApp - nassnav.com </footer>
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->


